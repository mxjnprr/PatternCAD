<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-patterncad">
        <comment>PatternCAD Document</comment>
        <comment xml:lang="fr">Document PatternCAD</comment>
        <comment xml:lang="de">PatternCAD-Dokument</comment>
        <comment xml:lang="es">Documento PatternCAD</comment>
        <glob pattern="*.patterncad"/>
        <magic priority="50">
            <match type="string" offset="0" value="PATTERNCAD"/>
        </magic>
        <icon name="patterncad"/>
        <generic-icon name="application-x-patterncad"/>
    </mime-type>
</mime-info>
